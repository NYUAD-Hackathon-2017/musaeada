<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="26dp"
    android:height="26dp"
    android:viewportHeight="26"
    android:viewportWidth="26">

    <group
        android:name="checkbox_filled_group"
        android:pivotX="13"
        android:pivotY="13">

        <path
            android:name="checkbox_filled_path"
            android:fillAlpha="0"
            android:fillColor="@color/colorAccent"
            android:pathData="@string/checkbox_filled_path" />
    </group>

    <group
        android:name="checkbox_group"
        android:pivotX="13"
        android:pivotY="13">
        <path
            android:name="checkbox_path"
            android:pathData="@string/checkbox_path"
            android:strokeColor="@color/colorAccent"
            android:strokeLineCap="round"
            android:strokeWidth="2" />
    </group>

    <group
        android:name="tick_group"
        android:pivotX="13"
        android:pivotY="13">
        <path
            android:name="tick_path"
            android:pathData="@string/tick_path"
            android:strokeAlpha="0"
            android:strokeColor="#fff"
            android:strokeLineCap="round"
            android:strokeWidth="2" />
    </group>

</vector>