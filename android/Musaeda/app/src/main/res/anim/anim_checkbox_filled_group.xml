<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">

    <objectAnimator
        android:duration="50"
        android:interpolator="@anim/file_interpolator"
        android:propertyName="scaleX"
        android:valueFrom="0"
        android:valueTo="0"
        android:valueType="floatType"/>

    <objectAnimator
        android:duration="50"
        android:interpolator="@anim/file_interpolator"
        android:propertyName="scaleY"
        android:valueFrom="0"
        android:valueTo="0"
        android:valueType="floatType"/>

    <objectAnimator
        android:duration="300"
        android:interpolator="@anim/file_interpolator_bounce"
        android:propertyName="scaleX"
        android:valueFrom="0"
        android:valueTo="1"
        android:valueType="floatType"
        android:startOffset="50"/>

    <objectAnimator
        android:duration="300"
        android:interpolator="@anim/file_interpolator_bounce"
        android:propertyName="scaleY"
        android:valueFrom="0"
        android:valueTo="1"
        android:valueType="floatType"
        android:startOffset="50"/>

</set>

